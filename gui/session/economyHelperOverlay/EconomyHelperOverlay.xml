<?xml version="1.0" encoding="utf-8"?>

<object
        name="economyHelper_Overlay"
        type="image"
        sprite="sessionOverlayBackground"
        size="-3 36 15% 10%"
        hidden="false"
        hotkey="session.economyhelper.overlay"
>

    <object size="0 0 100% 100%" name="economyHelper_resourceGatherRates">
        <object name="economyHelper_resourceGatherRates_Headline" type="text" size="0 0 100% 25"
                textcolor="255 255 255">
            Resource Gather Rates
        </object>
        <object size="0 25 100% 100%" name="economyHelper_resourceGatherRates_Counter">
            <repeat count="4">
                <object name="economyHelper_resource[n]" size="0 0 89 100%" type="button" style="resourceCounter"
                        tooltip_style="sessionToolTipBold">
                    <object name="economyHelper_resource[n]_icon" size="0 -2 40 38" type="image" ghost="true"/>
                    <object name="economyHelper_resource[n]_gather_rate" size="34 0 100%-2 100%-2" type="text"
                            style="resourceText"/>
                </object>
            </repeat>
        </object>
    </object>
</object>
